<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
  <servlet>
    <servlet-name>jx</servlet-name>
    <servlet-class>org.sapid.JXServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>jsx</servlet-name>
    <servlet-class>org.sapid.JSXServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>jx</servlet-name>
    <url-pattern>/jx</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>jsx</servlet-name>
    <url-pattern>/jsx</url-pattern>
  </servlet-mapping>

</web-app>
